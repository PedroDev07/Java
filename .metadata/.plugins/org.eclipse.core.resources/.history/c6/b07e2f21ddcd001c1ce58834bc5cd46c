package com.dio;

public class Printando {

	public static void main (string[] args) {
		
		
		
	}
	
	
	
	
	
	
	public static void nome(String[] args) {
		System.out.println("Pedro Lucas");
		System.out.println("Correia Rocha");	
	
	}
	
	public static void soma(String[] args) {
		int a = 2;
		int b = 3;
		System.out.println("a = " +a+ "/b = "+b+"/ e a soma = " +(a+b));			
		
	}

